<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.my2048game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".User.ChangePassword"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.MainPegSolitaire"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".User.Settings"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.ScoreGames"
            android:screenOrientation="portrait"
            android:exported="true" />
        <!-- <activity -->
        <!-- android:name=".DBHelper" -->
        <!-- android:exported="true" /> -->
        <activity
            android:name=".User.Register"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".User.LogIn"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.SplashGameOver"
            android:screenOrientation="portrait"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Games.SplashVictory"
            android:screenOrientation="portrait"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Games.MenuJuegos"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.Splash"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Games.Main2048"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.Game2048"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.GamePeg"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".Games.DetailActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
    </application>

</manifest>